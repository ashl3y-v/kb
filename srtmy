x l d g w  q b o u ;
s r t m y  p n a i e
z j k c v  f h . , /

default partial alphanumeric_keys
xkb_symbols "srtmy" {
    include "us(basic)"

    name[Group1]= "srtmy layout (US)";

    key <AD01>	{[ x, X ]};
    key <AD02>	{[ l, L ]};
    key <AD03>	{[ d, D ]};
    key <AD04>	{[ g, G ]};
    key <AD05>	{[ w, W ]};
    key <AD06>	{[ q, Q ]};
    key <AD07>	{[ b, B ]};
    key <AD08>	{[ o, O ]};
    key <AD09>	{[ u, U ]};
    key <AD10>	{[ semicolon, colon ]};

    key <AC01>	{[ s, S ]};
    key <AC02>	{[ r, R ]};
    key <AC03>	{[ t, T ]};
    key <AC04>	{[ m, M ]};
    key <AC05>	{[ y, Y ]};
    key <AC06>	{[ p, P ]};
    key <AC07>	{[ n, N ]};
    key <AC08>	{[ a, A ]};
    key <AC09>	{[ i, I ]};
    key <AC10>	{[ e, E ]};

    key <AB01>	{[ z, Z ]};
    key <AB02>	{[ j, J ]};
    key <AB03>	{[ k, K ]};
    key <AB04>	{[ c, C ]};
    key <AB05>	{[ v, V ]};
    key <AB06>	{[ f, F ]};
    key <AB07>	{[ h, H ]};
};
